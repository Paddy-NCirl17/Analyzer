interface=wlan0
driver=nl80211
ssid=Packet_Analyzer
hw_mode=g
channel=7
wmm_enabled=0
macaddr_acl=0
auth_algs=1
ignore_broadcast_ssid=0
wpa=2
wpa_passphrase=password
wpa_key_mgmt=WPA-PSK
wpa_pairwise=TKIP
rsn_pairwise=CCMP
sudo ifconfig eth0 192.168.8.1 netmask 255.255.255.0 broadcast 192.168.8.255
sudo iptables -t nat -A POSTROUTING -s 192.168.4.2/24 -o wlan0 -j MASQUERADE
sudo iptables -t nat -A POSTROUTING -s 192.168.8.0/24 -o wlan0 -j MASQUERADE